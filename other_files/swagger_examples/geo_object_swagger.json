  "host": "localhost:4567",
  "tags": [
    {
      "name": "GEO Object"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/continentes": {
      "get": {
        "summary": "List of continents",
        "operationId": "continentBO.findAll(name, start, maxresults)",
        "produces": [
          "application/json"
        ],
        "parameters": [          
          {
            "name": "name",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "start",
            "in": "query",
            "required": false,
            "type": "integer"
          },
          {
            "name": "maxresults",
            "in": "query",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/org.geoobject.model.Continent"
            }
          }
        }
      }
    },
    "/continentes/{id}": {
      "get": {
        "summary": "Continent by ID",
        "operationId": "continentBO.findById(id)",
        "produces": [
          "application/json"
        ],
        "parameters": [          
          {
            "name": "id",
            "in": "path",            
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/org.geoobject.model.Continent"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "org.geoobject.model.Continent": {
      "type": "object",
      "properties": {
        "id": {
          "type": "String"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "ApiError": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}