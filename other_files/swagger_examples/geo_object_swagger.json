  "paths": {
    "/continentes": {
      "post": {
        "summary": "Listado de continentes",
        "operationId": "continenteBO.findAll(id, continente)",
        "produces": [
          "application/json"
        ],
		"parameters": [
          {
            "name": "id",
            "in": "query",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "continente",
            "required": true,
            "schema": {
              "$ref": "#/definitions/org.geoobject.model.Continente"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Continente"
            }
          },
          "400": {
            "description": "Invalid input data",
            "schema": {
              "$ref": "#/definitions/ApiError"
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/ApiError"
            }
          },
          "404": {
            "description": "User not found",
            "schema": {
              "$ref": "#/definitions/ApiError"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "org.geoobject.model.Continente": {
      "type": "object",
      "properties": {
        "id": {
          "type": "String"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "ApiError": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
